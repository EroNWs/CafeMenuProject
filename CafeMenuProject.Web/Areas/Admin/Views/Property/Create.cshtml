@model CafeMenuProject.ViewModel.Concrete.Create.PropertyCreateVM
@{

	Layout = null;
}


<!DOCTYPE html>
<html>

<head>

	<!-- Basic -->
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<!-- Mobile Metas -->
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
	<!-- Site Metas -->
	<title>@ViewData["Title"] - GourmetDelice</title>
	<meta name="keywords" content="" />
	<meta name="description" content="" />
	<meta name="author" content="" />
	<link rel="shortcut icon" href="images/favicon.png" type="">

	<title> Gourmet Delice </title>

	<!-- bootstrap core css -->
	<link href="~/css/bootstrap.css" type="text/css" rel="stylesheet" />


	<!--owl slider stylesheet -->
	<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />
	<!-- nice select  -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-nice-select/1.1.0/css/nice-select.min.css" integrity="sha512-CruCP+TD3yXzlvvijET8wV5WxxEh5H8P4cmz0RFbKK6FlZ2sYl3AEsKlLPHbniXKSrDdFewhbmBK5skbdsASbQ==" crossorigin="anonymous" />
	<!-- font awesome style -->

	<link href="~/css/font-awesome.min.css" rel="stylesheet" />
	<!-- Custom styles for this template -->
	<link href="~/css/style.css.map" rel="stylesheet" />
	<link href="~/css/style.css" rel="stylesheet" />

	<!-- responsive style -->
	<link href="~/css/responsive.css" rel="stylesheet" />

	<style>
		body {
			background: white;
		}
		/* entypo */
		[class*="entypo-"]:before {
			font-family: 'entypo', sans-serif;
		}
		/* fontawesome */
		[class*="fontawesome-"]:before {
			font-family: 'FontAwesome', sans-serif;
		}
		/* zocial */
		[class*="zocial-"]:before {
			font-family: 'zocial', sans-serif;
		}

		[class^="icon-"], [class*=" icon-"] {
			font-family: 'icomoon';
			speak: none;
			font-style: normal;
			font-weight: normal;
			font-variant: normal;
			text-transform: none;
			line-height: 1;
			/* Better Font Rendering =========== */
			-webkit-font-smoothing: antialiased;
			-moz-osx-font-smoothing: grayscale;
		}

		.icon-cloudy:before {
			content: "\e60f";
		}

		.icon-sun:before {
			content: "\e610";
		}

		.icon-cloudy2:before {
			content: "\e611";
		}

		* {
			-webkit-box-sizing: border-box;
			-moz-box-sizing: border-box;
			box-sizing: border-box;
		}

		ul {
			list-style-type: none;
			margin: 0;
			padding-left: 0;
		}

		h1 {
			font-size: 23px;
		}



		p {
			font-size: 15px;
		}

		a {
			text-decoration: none;
			font-size: 15px;
		}

			a:hover {
				text-decoration: underline;
			}



		.input-container {
			position: relative;
		}


		input[type=text]::-webkit-input-placeholder { /* WebKit browsers */
			color: black;
		}

		input[type=text]:-moz-input-placeholder { /* Mozilla Firefox 4 to 18 */
			color: black;
		}

		input[type=text]::-moz-input-placeholder { /* Mozilla Firefox 19+ */
			color: black;
		}

		input[type=text]:-ms-input-placeholder { /* Internet Explorer 10+ */
			color: black;
		}

		input[type=text]:focus {
			outline: none; /* removes the default orange border when focus */
			border: 1px solid black;
		}

		.input-icon {
			font-size: 22px;
			position: absolute;
			left: 31px;
			top: 10px;
		}

			.input-icon.password-icon {
				left: 35px;
			}


		.horizontal-list {
			margin: 0;
			padding: 0;
			list-style-type: none;
		}

			.horizontal-list li {
				float: left;
			}

		.clear {
			clear: both;
		}

		.icon {
			font-size: 25px;
		}

		.titular {
			display: block;
			line-height: 60px;
			margin: 0;
			text-align: center;
			border-top-left-radius: 5px;
			border-top-right-radius: 5px;
		}

		.button {
			display: block;
			width: 175px;
			line-height: 50px;
			font-size: 16px;
			font-weight: 700;
			text-align: center;
			margin: 0 auto;
			border-radius: 5px;
			-webkit-transition: background .3s;
			transition: background .3s;
		}

			.button:hover {
				text-decoration: none;
			}

		.arrow-btn-container {
			position: relative;
		}

		.arrow-btn {
			position: absolute;
			display: block;
			width: 60px;
			height: 60px;
			-webkit-transition: background .3s;
			transition: background .3s;
		}

			.arrow-btn:hover {
				text-decoration: none;
			}

			.arrow-btn.left {
				border-top-left-radius: 5px;
			}

			.arrow-btn.right {
				border-top-right-radius: 5px;
				right: 0;
				top: 0;
			}

			.arrow-btn .icon {
				display: block;
				font-size: 18px;
				border: 2px solid #fff;
				border-radius: 100%;
				line-height: 17px;
				width: 21px;
				margin: 20px auto;
				text-align: center;
			}

			.arrow-btn.left .icon {
				padding-right: 2px;
			}

		.profile-picture {
			border-radius: 100%;
			overflow: hidden;
			-webkit-box-sizing: content-box;
			-moz-box-sizing: content-box;
			box-sizing: content-box;
		}

		.big-profile-picture {
			margin: 0 auto;
			border: 5px solid #50597b;
			width: 150px;
			height: 150px;
		}

		.small-profile-picture {
			border: 2px solid #50597b;
			width: 40px;
			height: 40px;
		}


		.main-container {
			font-family: 'Ubuntu', sans-serif;
			width: 100%;
			height: 1725px;
			margin: 1em auto;
		}


		header {
			height: 80px;
		}

		.header-menu {
			font-size: 17px;
			line-height: 80px;
		}

			.header-menu li {
				position: relative;
				-webkit-transform: translateZ(0); /** To avoid a flash when hover messages **/
			}

		.header-menu-tab {
			padding: 0 27px;
			display: block;
			line-height: 74px;
			font-size: 17px;
			-webkit-transition: background .3s;
			transition: background .3s;
		}

			.header-menu-tab:hover {
				background: #FFBE33;
				border-bottom: 4px solid black;
				text-decoration: none;
			}

			.header-menu-tab .icon {
				padding-right: 15px;
			}

		.header-menu-number {
			position: absolute;
			line-height: 22px;
			padding: 0 6px;
			font-weight: 700;
			background: red;
			border-radius: 100%;
			top: 15px;
			right: 2px;
			-webkit-transition: all .3s linear;
			transition: all .3s linear;
		}

		.header-menu li:hover .header-menu-number {
			text-decoration: none;
			-webkit-transform: rotate(360deg);
			transform: rotate(360deg);
		}

		.profile-menu {
			float: right;
			height: 80px;
			padding-right: 20px;
		}

			.profile-menu p {
				font-size: 17px;
				display: inline-block;
				line-height: 76px;
				margin: 0;
				padding-right: 10px;
			}

			.profile-menu a {
				padding-left: 5px;
			}

				.profile-menu a:hover {
					text-decoration: none;
				}

		.small-profile-picture {
			display: inline-block;
			vertical-align: middle;
		}

		.block {
			margin-bottom: 25px;
			background: black;
			border-radius: 5px;
		}

	</style>

</head>
<body class="sub_page">

	<div class="main-container">

		<!-- HEADER -->
		<header class="block">
			<ul class="header-menu horizontal-list">
				<li>
					<a class="header-menu-tab" asp-area="Admin" asp-controller="ByProduct" asp-action="Index" style="color: white;"><span class="icon entypo-cog scnd-font-color"></span>ByProduct</a>
				</li>
				<li>
					<a class="header-menu-tab" asp-area="Admin" asp-controller="Drink" asp-action="Index" style="color: white;"><span class="icon fontawesome-user scnd-font-color"></span>Drink</a>
				</li>
				<li>
					<a class="header-menu-tab" asp-area="Admin" asp-controller="Hamburger" asp-action="Index" style="color: white;"><span class="icon fontawesome-envelope scnd-font-color"></span>Hamburger</a>

				</li>
				<li>
					<a class="header-menu-tab" asp-area="Admin" asp-controller="Menu" asp-action="Index" style="color: white;"><span class="icon fontawesome-envelope scnd-font-color"></span>Menu</a>
				</li>
				<li>
					<a class="header-menu-tab" asp-area="Admin" asp-controller="Sauce" asp-action="Index" style="color: white;"><span class="icon fontawesome-envelope scnd-font-color"></span>Sauce</a>
				</li>

				<li>
					<a class="header-menu-tab" href="#5" style="color: white;"><span class="icon fontawesome-star-empty scnd-font-color"></span>Messages</a>
					<a class="header-menu-number" href="#4" style="color: white;">5</a>
				</li>
			</ul>
			<div class="profile-menu">
				<p>Me <a href="#26"><span class="entypo-down-open scnd-font-color"></span></a></p>
				<div class="profile-picture small-profile-picture">
					<img width="40px" alt="Eron Picture" src="~/images/f0552575-c87c-4b84-8f73-fada161c4dfc.jpg" />
				</div>
			</div>
		</header>







		<div class="container col-md-12 p-10 pb-4">
			<div class="card shadow border-0 mt-4">
				<div class="card-header bg-warning bg-gradient ml-0 py-3">
					<div class="row">
						<div class="col-12 text-center">
							<h2 class="text-white py-2">Create ByProduct</h2>
						</div>
					</div>
				</div>
				<div class="card-body p-4">
					<form method="post" class="row">

						<div class="form-floating py-2 col-12">
							<input asp-for="Key" class="form-control border-0 shadow" placeholder="Key" />

							<span asp-validation-for="Key" class="text-danger"></span>
						</div>
						<div class="form-floating py-2 col-12 mt-4">
							<input asp-for="Value" class="form-control border-0 shadow" placeholder="Value" />

							<span asp-validation-for="Value" class="text-danger"></span>

						</div>



						<div class="col-6 mt-4">
							<button type="submit" class="btn btn-dark form-control">Create a New ByProduct</button>
						</div>
						<div class="col-6 mt-4">
							<a asp-area="Admin" asp-controller="ByProduct" asp-action="Index" class="btn btn-outline-secondary form-control">Back to ByProduct List</a>
						</div>


					</form>


				</div>
			</div>
			<br />






		</div>
</body>
</html>